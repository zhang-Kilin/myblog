@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Welcome to Kilin's home.</title>
    @Ctrip.Framework.MVC.IncludeHelper.Include("demo.css")
    @Ctrip.Framework.MVC.IncludeHelper.Include("jQuery")
    @Ctrip.Framework.MVC.IncludeHelper.Include("bgResize")
    @Ctrip.Framework.MVC.IncludeHelper.Include("fontFlex")
    @Ctrip.Framework.MVC.IncludeHelper.Include("load")
    @Ctrip.Framework.MVC.IncludeHelper.Include("loading")
</head>
<body id="page">
    <div class="container">
        <header>
            <h1> <span>Welcome to Kilin's home.</span></h1>
            <h2>I am working hard to building the <a id="licious" href="javascript:void(0)">website</a>.</h2>
        </header>
    </div>
    <div id="screen-animate" style="display:none">
        <img url="static/myblog/images/1/1.jpg" title="se·ren·i·ty" alt="" />
        <img url="static/myblog/images/1/2.jpg" title="com·po·sure" alt="" />
        <img url="static/myblog/images/1/3.jpg" title="e·qua·nim·i·ty" alt="" />
        <img url="static/myblog/images/1/4.jpg" title="bal·ance" alt="" />
        <img url="static/myblog/images/1/5.jpg" title="qui·e·tude" alt="" />
        <img url="static/myblog/images/1/6.jpg" title="re·lax·a·tion" alt="" />
    </div>
    <script type="text/javascript">
        $(function () {
            $("#screen-animate").bgResize({
                speed:"slow"
            });
            $("#licious").click(function () {
                $.load("home/licious");
            });
        });
        //$.loading({
        //    img: "/myblog/static/pub/images/loading.gif"
        //});
    </script>
</body>
</html>
